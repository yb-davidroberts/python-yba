{
    "code": "$code",
    "name": "$name",
    "details": {
        "cloudInfo": {
            "kubernetes": {
                "kubernetesImageRegistry": "$image_registry",
                "kubernetesProvider": "$cloud_provider",
                "kubernetesPullSecretContent": "$pull_secret",
                "kubernetesPullSecretName": "$pull_secret_name",
                "kubernetesImagePullSecretName": "$pull_secret_name"
            }
        }
    },
    "regions": [
        {
            "code": "$region_code",
            "name": "$region_name",
            "zones": [
                {
                    "code": "$zone_code",
                    "name": "$zone_name",
                    "details": {
                        "cloudInfo": {
                            "kubernetes": {
                                "kubernetesStorageClass": "$storage_class"
                            }
                        }
                    }
                }
            ]
        }
    ]
}